{
	"name": "gymclub-mobileapp",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"format": "prettier --write .",
		"dev:android": "expo start --android -c",
		"dev:ios": "expo start --ios -c",
		"dev-build:android": "expo run:android",
		"dev-build:ios": "expo run:ios",
		"prebuild": "expo prebuild",
		"prebuild:android": " expo prebuild --platform android",
		"prebuild:ios": "expo prebuild --platform ios",
		"test": "jest",
		"test:unit": "jest --config jest-unit-config.ts --watchAll",
		"test:integration": "jest --config jest-integration-config.ts --watchAll",
		"android": "expo run:android",
		"ios": "expo run:ios"
	},
	"dependencies": {
		"@expo-google-fonts/manrope": "^0.2.3",
		"@expo/vector-icons": "^14.0.0",
		"@gymclub/utils": "workspace:*",
		"@react-native-community/geolocation": "^3.3.0",
		"@react-navigation/bottom-tabs": "^6.6.0",
		"@react-navigation/native": "^6.0.2",
		"@stripe/stripe-react-native": "^0.38.6",
		"axios": "^1.7.2",
		"dotenv": "^16.4.5",
		"expo": "^51.0.22",
		"expo-build-properties": "~0.12.3",
		"expo-constants": "^16.0.2",
		"expo-dev-client": "~4.0.20",
		"expo-font": "~12.0.7",
		"expo-linking": "~6.3.1",
		"expo-router": "~3.5.15",
		"expo-secure-store": "~13.0.2",
